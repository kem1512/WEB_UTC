@model IEnumerable<Major>
<ul class="nav">
    <li class="nav-item" id="-1">
        <a class="nav-link">All</a>
    </li>
    @foreach (var m in Model)
    {
        <li class="nav-item" id="@m.MajorId">
            <a class="nav-link" href="javascript:void(0);">@m.MajorName</a>
        </li>
    }
</ul>
